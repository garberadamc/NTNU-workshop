TITLE:
T3RILTA - vary n-size
MONTECARLO:
NAMES = u11-u15 u21-u25 u31-u35;
GENERATE = u11-u15 u21-u25 u31-u35(1);
CATEGORICAL = u11-u15 u21-u25 u31-u35;
GENCLASSES = c1(2) c2(2) c3(2);
CLASSES = c1(2) c2(2) c3(2);
NOBSERVATIONS = 2500;
 SEED = 3252020;
	NREPS = 5; !rep number reduced for demo (real sim use 500,1000)
 repsave = all;
 save = 5_t3n500rep*.dat;
 RESULTS = t3results5.csv;
MODEL POPULATION:
%OVERALL%
      
      [c1#1-c3#1*0];      
  	  c2#1 on c1#1*0.5;    
      c3#1 on c2#1*0.5;

      f by u11-u15*2 (p1-p5)
           u21-u25*2 (p1-p5)
           u31-u35*2 (p1-p5);
           
    	f@1;
      [f@0];

 
  MODEL POPULATION-c1:
      %c1#1%
    	[u11$1*1] (p111);
    	[u12$1*1] (p211);
    	[u13$1*1] (p311);
    	[u14$1*1] (p411);
      [u15$1*1] (p511);

    	%c1#2%
    	[u11$1*-1] (p121);
    	[u12$1*-1] (p221);
    	[u13$1*-1] (p321);
    	[u14$1*-1] (p421);
      [u15$1*-1] (p521);

  MODEL POPULATION-c2:	
      %c2#1%
    	[u21$1*1] (p111);
    	[u22$1*1] (p211);
    	[u23$1*1] (p311);
    	[u24$1*1] (p411);
      [u25$1*1] (p511);

    	%c2#2%
    	[u21$1*-1] (p121);
    	[u22$1*-1] (p221);
    	[u23$1*-1] (p321);
    	[u24$1*-1] (p421);
      [u25$1*-1] (p521); 
    
  MODEL POPULATION-c3: 	
    	%c3#1%
    	[u31$1*1] (p111);
    	[u32$1*1] (p211);
    	[u33$1*1] (p311);
    	[u34$1*1] (p411);
      [u35$1*1] (p511);

    	%c3#2%
    	[u31$1*-1] (p121);
    	[u32$1*-1] (p221);
    	[u33$1*-1] (p321);
    	[u34$1*-1] (p421);
      [u35$1*-1] (p521);
ANALYSIS:
TYPE = MIXTURE;
       algorithm = integration;
    	 processors = 8;
MODEL:
%OVERALL%

     [c1#1-c3#1*0] (par1-par3);

  	c2#1 on c1#1*0.5 (par11);
    c3#1 on c2#1*0.5;


      f by u11-u15*2 (p1-p5)
           u21-u25*2 (p1-p5)
           u31-u35*2 (p1-p5);
    	f@1;
      [f@0];
     
  MODEL c1:

      %c1#1%
